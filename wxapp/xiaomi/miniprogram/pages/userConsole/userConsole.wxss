/* pages/userConsole/userConsole.wxss */
page{
  background-color: rgb(245, 245, 245);
}
h3{
  position: absolute;
  top:50rpx;
  left: 30rpx;
}
.title{
  display: flex;
  background-color: #FFFFFF;
  justify-content: space-around;
  height: 70rpx;
  text-align: center;
  font-size: smaller;
}
.show{

  width: 100%;
  height: 200px;
}
.nothing{
  height: 250rpx;
  width: 250rpx;
  border-radius: 60px;
  overflow: hidden;
  margin:0 auto;
  position: relative;
  top: 40px;
  
}
.nothing image{
  max-width: 100%;
  max-height: 100%;
}
.intrudaction button{
  position: relative;
  top: 50px;
   height: 34px;
   width:280px;
  margin: 10px auto;
  color: white;
  background-color:rgba(255,103,0);
}
.intrudaction text{
  position: relative;
  display: flex;
  justify-content: space-around;
  font-size:8px ;
  color: rgba(102, 100, 100, 0.5);
  position: relative;
  top: 48px;
}